{
  "name": "soketto-client",
  "version": "1.0.0-release",
  "author": "Tufan Meri√ß Uyguner",
  "license": "MIT",
  "keywords": [],
  "description": "",
  "browser": "dist/soketto-client.js",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.mjs",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/soketto-client.js",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "build",
    "lib",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build:rollup": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build:types": "dts-bundle-generator -o dist/soketto-client.d.ts src/index.ts",
    "build": "tsc && yarn build:rollup && yarn build:types && yarn typedoc"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^16.4.6",
    "dts-bundle-generator": "^8.0.1",
    "rollup": "^3.20.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.4",
    "typedoc-plugin-markdown": "^3.15.1",
    "typescript": "^5.0.3"
  },
  "packageManager": "yarn@3.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tufcode/soketto-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "paketto": "^0.0.1",
    "tslib": "^2.5.0",
    "ws": "^8.13.0"
  }
}
